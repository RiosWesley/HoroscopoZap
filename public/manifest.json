{
  "name": "HoroscopoZap",
  "short_name": "HoroscopoZap",
  "description": "Analise conversas do WhatsApp de forma rápida, segura e divertida. Faça upload de arquivos .zip ou .txt exportados do WhatsApp e descubra insights únicos!",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#c66dd6",
  "scope": "/",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/website-image.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },

    {
      "src": "/website-image.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "share_target": {
    "action": "/receive-share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["application/zip"]
        }
      ]
    }
  }
}
